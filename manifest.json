{
	"manifest_version": 2,
	"name": "Blogs-",
	"version": "1.0",
	"description": "An extension for blogs.dalton.org",

	"author": "Courses+ Dev Team",
	"homepage_url": "https://coursesplus.tk/blogsminus",
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"*://*.dalton.org"
	],
	"content_scripts": [
    {
      "matches": ["*://blogs.dalton.org/*"],
      "css": ["index.css"],
      "js": ["jquery.js", "inject.js"]
    }
  ],
}